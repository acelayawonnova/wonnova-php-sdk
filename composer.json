{
    "name": "wonnova/sdk",
    "description": "Wonnova SDK for PHP. Use Wonnova's RESTful API directly from your PHP projects",
    "license": "MIT",
    "authors": [
        {
            "name": "Wonnova Gamification",
            "email": "hello@wonnova.com"
        }
    ],
    "require": {
        "guzzle/guzzle": "~3.9",
        "netresearch/jsonmapper": "~0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "~1.5"
    },
    "autoload": {
        "psr-4": {
            "Wonnova\\SDK\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wonnova\\SDK\\Test\\": "src"
        }
    }
}
